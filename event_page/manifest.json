{
  "name": "UnBias",
  "description": "Click link to get more news",
  "version": "0.7",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "LinksPopUp.html"
  },
  "icons": { 
    "16":"icon.png", 
    "48":"icon.png",
    "128": "icon.png"
  },
  "permissions": ["tabs", "webNavigation", "history", "activeTab"],
  "background": {
    "persistent": false,
    "scripts": ["sample.js"]
  },
  "manifest_version": 2
}